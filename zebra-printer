# –ü–ï–ß–ê–¢ –ù–ê ZPL –ö–û–ú–ê–ù–î–ò–¢–ï
// –û—Ç–≤–∞—Ä—è–º–µ —Å–æ–∫–µ—Ç –∑–∞ —Å–≤—ä—Ä–∑–≤–∞–Ω–µ —Å –ø—Ä–∏–Ω—Ç–µ—Ä–∞
$ip = '192.168.1.7';
$port = 9100;
$fp = fsockopen($ip, $port, $errno, $errstr, 5);
if (!$fp) {
    http_response_code(500);
    echo "–ù–µ—É—Å–ø–µ—à–Ω–∞ –≤—Ä—ä–∑–∫–∞: $errstr ($errno)";
    exit;
}
// –ò–∑–ø—Ä–∞—â–∞–º–µ ZPL –∫–æ–º–∞–Ω–¥–∞—Ç–∞
fwrite($fp, $zpl);
fclose($fp);  // –ó–∞—Ç–≤–∞—Ä—è–º–µ —Å–æ–∫–µ—Ç–∞
// –ü–æ—Ç–≤—ä—Ä–∂–¥–∞–≤–∞–º–µ —É—Å–ø–µ—à–µ–Ω –ø–µ—á–∞—Ç
echo "OK";
